{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "sj_basic4",
  "version": "4.0.0",
  "description": "SJ Basic4 template for Joomla 4",
  "license": "GPL-2.0-or-later",
  "assets": [
    {
      "name": "template.sj_basic4.ltr",
      "type": "style",
      "uri": "template.min.css",
      "dependencies": [
        "fontawesome",
        "template.sj_basic4.googlefont"
      ]
    },
    {
      "name": "template.sj_basic4.rtl",
      "type": "style",
      "uri": "template-rtl.min.css",
      "dependencies": [
        "fontawesome",
        "template.sj_basic4.googlefont"
      ]
    },
    {
      "name": "template.offline",
      "type": "style",
      "uri": "offline.css"
    },
    {
      "name": "template.active.language",
      "type": "style",
      "uri": "",
      "class": "LangActiveAssetItem",
      "dependencies": [
        "template.active"
      ]
    },
    {
      "name": "template.user",
      "type": "style",
      "uri": "user.css",
      "dependencies": [
        "template.active",
        "template.active.language"
      ]
    },
    {
      "name": "template.icomoon",
      "type": "style",
      "uri": "font-icomoon.css"
    },
	{
      "name": "template.sj-basic4",
      "type": "style",
      "uri": "sj-basic4.css",
      "dependencies": [
        "fontawesome",
        "template.sj_basic4.googlefont"
      ]
    },
    {
      "name": "template.sj_basic4.googlefont",
      "type": "style",
      "uri": "https://fonts.googleapis.com/css?family=Open+Sans:400"
    },
    {
      "name": "template.sj_basic4",
      "type": "script",
      "uri": "template.js",
      "dependencies": [
        "core",
        "bootstrap.js.bundle"
      ]
    },
    {
      "name": "template.active",
      "type": "script",
      "uri": "",
      "description": "A dummy asset to allow extensions to use it as a dependency to the active template",
      "dependencies": [
        "template.sj_basic4"
      ]
    },
    {
      "name": "template.user",
      "type": "script",
      "uri":  "user.js",
      "dependencies": [
        "template.active"
      ]
    },	
    {
      "name": "template.sj_basic4.ltr",
      "type": "preset",
      "dependencies": [
        "template.sj_basic4.ltr#style",
        "template.sj_basic4#script"
      ]
    },
    {
      "name": "template.sj_basic4.rtl",
      "type": "preset",
      "dependencies": [
        "template.sj_basic4.rtl#style",
        "template.sj_basic4#script"
      ]
    },
    {
      "name": "searchtools",
      "type": "style",
      "uri": "system/searchtools/searchtools.min.css"
    }
  ]
}
